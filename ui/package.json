{
  "name": "Augmentedjs/tiledpalettequant",
  "version": "1.0.0",
  "description": "Tile-aware palette quantization for retro consoles (e.g., Sega Genesis / Mega Drive).",
  "main": "index.tsx",
  "type": "module",
  "scripts": {
    "start": "NODE_ENV=development webpack serve",
    "dev": "NODE_ENV=development webpack --mode development",
    "build": "NODE_ENV=production webpack --mode production",
    "test": "jest",
    "clean": "rm -rf node_modules",
    "reinstall": "npm run clean && npm install --verbose",
    "rebuild": "npm run clean && npm install --verbose && npm run build",
    "lint": "eslint 'src/**/*.{js,ts,tsx}'",
    "format": "prettier --write 'src/**/*.{js,ts,tsx,json,css}'"
  },
  "keywords": [
    "react",
    "typescript",
    "mongodb",
    "google-cloud",
    "app-engine"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Augmentedjs/tiledpalettequant.git"
  },
  "author": "Bob Warren <jedi.master@augmentedjs.com>",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=22",
    "npm": ">=10"
  },
