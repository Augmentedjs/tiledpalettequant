{
  "presets": [
    ["@babel/preset-env", {
      "targets": {
        "browsers": ["firefox >= 120", "chrome >= 120", "ios >= 18"]
      },
      "debug": true,
      "modules": false
    },
    "@babel/preset-react"
    ]
  ],
  "plugins": [
    "@babel/plugin-transform-react-jsx",
    ["transform-class-properties", { "spec": true }],
    "babel-plugin-remove-template-literals-whitespace"
  ],
  "comments": false,
  "env": {
    "production": {
      "plugins": [
        ["transform-remove-console", { "exclude": [ "error", "warn"] }]
      ]
    },
    "test": {
      "plugins": [
        ["transform-remove-console", { "exclude": [ "info", "error", "warn"] }]
      ],
      "presets": [
        ["@babel/preset-env", {
          "targets": {
            "node": "20"
          },
          "debug": true,
          "modules": false
        },
        "@babel/preset-react"
        ]
      ]
    }
  }
}
